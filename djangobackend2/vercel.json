{
    "version":2,
    "builds": [{
        "src": "djangobackend2/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
        "src": "build.sh",
        "use": "@vercel/static.build",
        "config": { "distDir": "staticfiles_build" }
    }],

    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "djangobackend2/wsgi.py"
        },
        {
            "src": "/(.*)",
            "dest": "djangobackend2/wsgi.py"
        }
    ]
    
    
    

    

}